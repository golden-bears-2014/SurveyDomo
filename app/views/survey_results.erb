
<div class="container"> 

  <script type="text/javascript">
  var answers = <%= @answers.to_json %>
  </script>

<h2><%= @survey.name %> Results</h2>

  <% @questions.each do |question| %>
    <script type="text/javascript">
    var question = <%= question.to_json %>
    var choices = <%= question.choices.to_json %>
    printGraph(question, answers, choices);
    </script>
  <% end %>
 

</div>





